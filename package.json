{
	"name": "tirgus-ui",
	"version": "1.0.0",
	"description": "UI for tirgus marketplace ui",
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "jest",
		"start": "webpack-dev-server --mode development ",
		"format": "prettier --write \"src/**/*.js\"",
		"eslint-fix": "eslint --fix \"src/**/*.js\"",
		"build": "webpack --mode production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EthanDavis/tirgus-ui.git"
	},
	"keywords": [
		"react"
	],
	"author": "Ethan Davis",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/EthanDavis/tirgus-ui/issues"
	},
	"homepage": "https://github.com/EthanDavis/tirgus-ui#readme",
	"jest": {
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"collectCoverageFrom": [
			"src/**/*.js",
			"!src/index.js"
		],
		"coverageReporters": [
			"text"
		]
	},
	"dependencies": {
		"@babel/runtime": "^7.9.2",
		"@fortawesome/fontawesome-svg-core": "^1.2.28",
		"@fortawesome/free-brands-svg-icons": "^5.13.0",
		"@fortawesome/free-solid-svg-icons": "^5.13.0",
		"@fortawesome/react-fontawesome": "^0.1.9",
		"axios": "0.19.2",
		"bootstrap": "4.4.1",
		"jquery": "^3.5.1",
		"prop-types": "^15.7.2",
		"react": "16.9.0",
		"react-dom": "16.9.0",
		"react-router-dom": "5.1.2",
		"react-toastify": "^5.5.0"
	},
	"devDependencies": {
		"@babel/core": "7.5.5",
		"@babel/plugin-proposal-class-properties": "7.8.3",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/preset-env": "7.5.5",
		"@babel/preset-react": "7.0.0",
		"babel-eslint": "10.0.2",
		"babel-jest": "25.1.0",
		"babel-loader": "8.0.6",
		"css-loader": "3.2.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.2",
		"enzyme-to-json": "3.4.4",
		"eslint": "6.2.1",
		"eslint-config-react": "1.1.7",
		"eslint-loader": "2.2.1",
		"eslint-plugin-react": "7.14.3",
		"html-webpack-plugin": "3.2.0",
		"jest": "25.1.0",
		"less": "3.10.3",
		"less-loader": "5.0.0",
		"prettier": "1.18.2",
		"style-loader": "1.0.0",
		"svg-inline-loader": "^0.8.2",
		"webpack": "4.39.2",
		"webpack-cli": "3.3.7",
		"webpack-dev-server": "3.8.0"
	}
}
